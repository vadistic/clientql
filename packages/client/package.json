{
  "private": false,
  "name": "@clientql/client",
  "description": "Generate strongly typed, flexible & fast client SDK from any GraphQL API",
  "license": "MIT",
  "author": "Jakub Wadas <vadistic@gmail.com>",
  "repository": "https://github.com/vadistic/clientql",
  "version": "0.1.0",
  "source": "src/index.ts",
  "types": "dist/types/index.d.ts",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs && yarn build:types",
    "build:cjs": "tsc -p tsconfig.build.json -m commonjs --outDir dist/cjs",
    "build:esm": "tsc -p tsconfig.build.json -m esnext --outDir dist/esm",
    "build:types": "tsc -p tsconfig.build.json --emitDeclarationOnly -d true --declarationMap --outDir dist/types",
    "clean": "rm -rf dist",
    "format": "prettier --write src/*",
    "lint": "eslint --ext ts src",
    "test": "jest",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@clientql/core": "0.1.0",
    "apollo-client": "2.6.8",
    "graphql": "14.6.0"
  },
  "optionalDependencies": {
    "graphql": "14.6.0"
  },
  "devDependencies": {
    "@clientql/testing": "0.1.0",
    "typescript": "3.8.3"
  }
}
