{
  "private": false,
  "name": "@clientql/cli",
  "description": "Generate strongly typed, flexible & fast client SDK from any GraphQL API",
  "license": "MIT",
  "author": "Jakub Wadas <vadistic@gmail.com>",
  "repository": "https://github.com/vadistic/clientql",
  "version": "0.1.0",
  "main": "dist/cjs/index.js",
  "bin": {
    "clientql": "./dist/cjs/cli.js"
  },
  "scripts": {
    "build": "yarn build:cjs && yarn build:types",
    "build:cjs": "tsc -p tsconfig.build.json -m commonjs --outDir dist/cjs",
    "build:types": "tsc -p tsconfig.build.json --emitDeclarationOnly -d true --declarationMap --outDir dist/types",
    "clean": "rm -rf dist",
    "prepare": "yarn clean && yarn build",
    "start": "ts-node src/cli.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "types": "dist/types/index.d.ts",
  "dependencies": {
    "@clientql/codegen": "^0.1.0",
    "@clientql/core": "^0.1.0",
    "fs-extra": "^8.1.0",
    "graphql": "^14.5.8",
    "meow": "^5.0.0",
    "node-fetch": "^2.3.0"
  },
  "devDependencies": {
    "@clientql/testing": "^0.1.0",
    "@types/fs-extra": "^8.0.0",
    "@types/jest": "^24.0.18",
    "@types/meow": "^5.0.0",
    "@types/node": "^12.7.11",
    "@types/node-fetch": "^2.3.2",
    "typescript": "^3.6.3"
  },
  "source": "src/index.ts"
}
