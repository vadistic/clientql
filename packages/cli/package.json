{
  "name": "@graphql-clientgen/cli",
  "version": "0.1.0",
  "private": false,
  "repository": "https://github.com/vadistic/graphql-client-generator",
  "author": "Jakub Wadas <vadistic@gmail.com>",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "source": "src/index.ts",
  "bin": {
    "gql-clientgen": "./dist/cjs/cli.js"
  },
  "scripts": {
    "build:esm": "tsc -p tsconfig.build.json -m esnext --outDir dist/esm",
    "build:cjs": "tsc -p tsconfig.build.json -m commonjs --outDir dist/cjs",
    "build:types": "tsc -p tsconfig.build.json --emitDeclarationOnly -d true --declarationMap --outDir dist/types",
    "build": "yarn build:esm && yarn build:cjs && yarn build:types",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "clean": "rm -rf dist",
    "prepare": "yarn clean && yarn build",
    "test": "jest"
  },
  "dependencies": {
    "@graphql-clientgen/core": "^0.1.0",
    "fs-extra": "^7.0.1",
    "graphql": "^14.2.1",
    "meow": "^5.0.0",
    "node-fetch": "^2.3.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.5",
    "@types/graphql": "^14.2.0",
    "@types/jest": "24.0.11",
    "@types/node": "11.13.0",
    "@types/node-fetch": "^2.3.0",
    "typescript": "3.4.1"
  }
}
