{
  "private": false,
  "name": "@graphql-clientgen/cli",
  "license": "MIT",
  "author": "Jakub Wadas <vadistic@gmail.com>",
  "repository": "https://github.com/vadistic/graphql-client-generator",
  "version": "0.1.0",
  "main": "dist/cjs/index.js",
  "bin": {
    "clientgen": "./dist/cjs/cli.js"
  },
  "scripts": {
    "build": "yarn build:cjs && yarn build:types",
    "build:cjs": "tsc -p tsconfig.build.json -m commonjs --outDir dist/cjs",
    "build:types": "tsc -p tsconfig.build.json --emitDeclarationOnly -d true --declarationMap --outDir dist/types",
    "clean": "rm -rf dist",
    "prepare": "yarn clean && yarn build",
    "start": "ts-node src/cli.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "types": "dist/types/index.d.ts",
  "dependencies": {
    "@graphql-clientgen/codegen": "^0.1.0",
    "@graphql-clientgen/core": "^0.1.0",
    "@graphql-clientgen/generator": "^0.1.0",
    "fs-extra": "^7.0.1",
    "graphql": "^14.2.1",
    "meow": "^5.0.0",
    "node-fetch": "^2.3.0"
  },
  "devDependencies": {
    "@graphql-clientgen/testing": "^0.1.0",
    "@types/fs-extra": "^5.0.5",
    "@types/graphql": "^14.2.0",
    "@types/jest": "24.0.11",
    "@types/meow": "^5.0.0",
    "@types/node": "11.13.4",
    "@types/node-fetch": "^2.3.2",
    "typescript": "3.6.3"
  },
  "source": "src/index.ts"
}
